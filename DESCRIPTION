Package: WINS
Type: Package
Title: Crop Stress Weather Information Delivery System
Version: 0.0.1.9000
Authors@R: c(person("Adam", "Sparks", role = c("aut", "cre"),
    email = "adamhsparks@gmail.com",
    comment = "https://orcid.org/0000-0002-0061-8359"),
    person("Keith", "Pembleton", role = "aut",
