
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{WINS}: Crop Stress Weather Information Delivery System in R",
         author = c(person("Adam", "Sparks"),
                    person("Keith", "Pembleton"),
                    person("Gordon", "Grundy")),
         year = year,
         note = note,
         url = "https://github.com/ToowoombaTrio/WINS")
